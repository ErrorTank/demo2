.logo{
  display flex
  margin-top 10px
  align-content center
  .circle-img{
    width 60px
    height 60px
    border 6px solid grey
    overflow hidden
    position relative
    border-radius 50%
    img{
      position absolute
      top 50%
      left 50%
      transform translate(-50%, -50%)
      width 50px
    }
  }
  .logo-label{
    color #4d4d4d
    font-size sm + 0.2rem
    font-weight bold
    margin-left 15px
    line-height 50px
  }
}
