.date-input{
  .input-contain{
    width 88%
    display inline-block
  }
  .date-picker-toggle{
    width 12%
    display inline-block
    padding 6px 10px
    text-align center
    background-color #f3f3f3
    border 1px solid grey
    border-left none
    border-bottom-right-radius 3px
    font-size 1.6rem
    border-top-right-radius 3px
    cursor pointer
    color black
  }
}
