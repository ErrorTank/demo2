.venue-map-item{
  margin-top 20px
  padding 30px
  border 1px solid grey
  background-color white
  border-radius 4px
  .upload-placeholder{
    color #505050
    font-size 1.4rem
    font-weight 550
  }
  .venue-map-name{
    margin-top 20px
    border-radius  4px
    border 1px solid grey
    outline none
    padding 5px 14px
    font-size sm - 0.2rem
    color #687089
    width 100%
  }
  .footer{
    margin-top 15px
    .left{
      width 50%
      display inline-block
      vertical-align top
      text-align left
      .title{
        font-size sm - 0.2rem
        margin-bottom 8px
        color black
      }
    }
    .right{
      width 50%
      display inline-block
      vertical-align top
      text-align right
      .title{
        font-size sm - 0.2rem
        margin-bottom 8px
        color black
      }
      .del-map{
        color #7dbc36
        cursor pointer
        font-size sm - 0.3rem
      }
    }
  }
}
