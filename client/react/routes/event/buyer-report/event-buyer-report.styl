.event-buyer-report{
  .route-header{
    padding 15px 25px
    justify-content space-between
    align-items center
    display flex
    .title{
      color #4d4d4d
      font-weight bold
      font-size md
    }
  }
  .route-body{
    padding 30px 50px
    text-align right
    .table-wrap{
      margin-top 15px

    }
    .action-panel{
      .search-opt-ins{
        display inline-block
        width 32%
        margin-right 1.3%
        background-color white
        vertical-align top
      }
      .select-opt-ins{
        display inline-block
        width 32%
        text-align left
        background-color white
        vertical-align top

      }
    }
  }
}
